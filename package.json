    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@mojaloop/central-services-logger": "10.6.0",
    "mongoose": "5.9.23"
  },
  "devDependencies": {
    "eslint": "7.4.0",
    "nyc": "15.1.0",
